ARGCMDS=1	   #commands with args
PATH=1		   #commands using PATH(others do not use PATH unless specified)
PATHARGCMDS=1	   #commands using PATH and args
SEMICOLON=1	   #using the semicolon in line (not incl quotes)
COMMENT=1	   #having comments in line
